{
  "name": "@true-directive/material-grid",
  "description": "Material design wrapper for the TrueDirective Grid.",
  "version": "1.0.1",
  "scripts": {
    "test": "karma start",
    "test-ci": "karma start --singleRun --browsers ChromeHeadless",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "build:cl": "ncp CHANGELOG.md dist/CHANGELOG.md",
    "build:lic": "ncp LICENSE dist/LICENSE",
    "build:themes_scss": "ncp ./styles/ dist/themes/",
    "build": "npm run clean && ngc && ng-packagr -p ng-package.json && npm run build:themes_scss && npm run build:cl && npm run build:lic"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/true-directive/material-grid"
  },
  "keywords": [
    "angular",
    "angular material",
    "grid",
    "data grid",
    "datatable",
    "data sorting",
    "data filtering",
    "data summaries",
    "inline editing",
    "virtual scrolling",
    "formatted input"
  ],
  "author": "Aleksey Melnikov <m_alx@list.ru>",
  "license": "MIT",
  "main": "./bundles/true-directive-material-grid.umd.js",
  "module": "./true-directive-material-grid.es5.js",
  "typings": "./true-directive-material-grid.d.ts",
  "homepage": "https://truedirective.com/demo/material",
  "dependencies": {
    "@true-directive/grid": "1.4.0"
  },
  "devDependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.4.0",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "core-js": "^3.5.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^3.4.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "ncp": "^2.0.0",
    "ng-packagr": "~5.3.0",
    "node-sass": "^4.13.0",
    "raw-loader": "^3.1.0",
    "rxjs": "^6.5.2",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^6.0.4",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "3.2.2",
    "webpack": "^4.39.3",
    "zone.js": "~0.8.26"
  },
  "engines": {
    "npm": ">4.0.0"
  }
}
